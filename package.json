{
	"name": "hoshimi",
	"version": "0.1.0",
	"description": "A lavalink@v4 client easy to use, up-to-date.",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"packageManager": "pnpm@9.15.1+sha512.1acb565e6193efbebda772702950469150cf12bcc764262e7587e71d19dc98a423dff9536e57ea44c49bdf790ff694e83c27be5faa23d67e0c033b583be4bfcf",
	"files": ["dist"],
	"lint-staged": {
		"*.{ts,json}": ["biome format --write ."]
	},
	"repository": {
		"url": "https://github.com/Ganyu-Studios/Hoshimi.git",
		"type": "git"
	},
	"scripts": {
		"build": "tsup",
		"lint": "biome lint ./src",
		"typecheck": "tsc --noEmit",
		"pkg:prepublish": "pnpm build && tsx ./scripts/prepublish.mts && cd .npm && npm pack",
		"pgk:publish": "cd .npm && npm publish",
		"format": "biome format --write .",
		"prepare": "pnpm build && husky"
	},
	"keywords": [
		"discord",
		"lavalink",
		"lavalink-v4",
		"discord",
		"distube",
		"music-bot",
		"lavalink-player",
		"lavalink-client",
		"music"
	],
	"author": "Ganyu Studios",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "^1.9.3",
		"@types/node": "^22.7.6",
		"@types/ws": "^8.5.13",
		"husky": "^9.1.6",
		"lint-staged": "^15.2.10",
		"tsup": "^8.3.0",
		"tsx": "^4.19.1",
		"typescript": "^5.6.3",
		"vitest": "^2.1.3"
	},
	"dependencies": {
		"ws": "^8.18.0"
	}
}
